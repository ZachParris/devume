<section class="projects-wrap">
  <div class="projects">
    <h2 class="section-head">Personal Projects</h2>
      {{#each projects}}
        <h6 class="section-sub-head">
          <a target="_blank" href="{{this.url}}" title="{{this.name}}"><strong>{{this.name}}</strong></a>
        </h6>
        <p class="summary">{{this.summary}}
        <a class="summary" href="{{this.link}}">Preview On GitHub</a>
        </p>
        <br>
        <img src="{{this.picture}}" alt="" />
        <br>
        <img src="{{this.picture2}}" alt="" />
      {{/each}}
  </div>
</section>
